/*----------------------------------------------
 * clearfix, overflow: hidden
 *--------------------------------------------*/
div#Contents,
div#LayoutA,
div#LayoutB,
div#LayoutC {
    zoom: 1;
    overflow: hidden;
    overflow: -moz-scrollbars-none;
}

#Left,
#Center,
#Top {
    zoom: 1;
    overflow: hidden;
}

dd div, dt, ol, td, th, p,
h1, h2, h3, h4, h6, .partsHeading,
fieldset, label {
    overflow: hidden;
}

dd div div {
    overflow: visible;
}

pre {
    overflow: auto;
}

