/*pagination*/
.wrapper-pagination {
}
ul.pagination {
    border: 0 none;
    list-style-type: none;
    margin-right: 20px;
    padding: 0;
}
ul.pagination li a img, ul.pagination li a:hover img {
    border: medium none !important;
}
.pagination li {
    border: 0 none;
    float: left;
    font-size: 11px;
    list-style: none outside none;
    margin: 0;
    padding: 0;
}
.pagination a {
    margin-right: 2px;
}
* html .pagination a {
    margin-right: 1px;
}
.pagination a:link, .pagination a:visited {
    color: #333333;
    display: block;
    float: left;
    padding: 2px 6px;
    text-decoration: none;
}
.pagination a:hover {
    border: 1px solid #DADADA;
    color: #014C8F;
    padding: 1px 5px 3px;
}
.pagination .active {
    background: none repeat scroll 0 0 #4480b1;
    color: #FFFFFF!important;
    display: block;
    float: left;
    font-weight: bold;
    margin-right: 2px;

}
.pagination a:link img, .pagination a:visited img {
    display: block;
    float: left;
    padding: 3px 1px 4px 0;
    text-decoration: none;
}
.pagination .next a, .pagination .previous a {
    background-color: #F3F3F3;
    border: 1px solid #DADADA;
}
.pagination .next a:hover img, .pagination .previous a:hover img {
    border: medium none;
    padding: 4px 2px 3px 1px;
}
.pagination .next a {
    margin-right: 0;
}
.pagination .previous-off img, .pagination .next-off img {
    background-color: #F3F3F3;
    border: 1px solid #DADADA;
    display: block;
    float: left;
    font-weight: bold;
    margin-right: 2px;
    padding: 5px 6px 6px;
}



