@charset "UTF-8";

/* ************************************* */
/* Common styles for the theme           */
/* ************************************* */
/* Normal, standard links. */
.hidden {
    display: none !important;
}
a:link {  color: #37538c !important;	}
a:visited {  color: #37538c;	}
a:link{		text-decoration: none;}
a:visited{	text-decoration: none;}
a:hover {text-decoration: none;}
table{
    empty-cells: show;
    border-collapse: collapse;
    border-spacing: 0;
}

#main-wrap {
    /*border-bottom: 1px solid #C8C8C8;*/
}

body {
    font: Verdana, Arial, Helvetica, sans-serif;
}

.dashboard_main_wrap  {
    padding-bottom: 20px;
}


/* set a fontsize that will look the same in all browsers.
body*/
div,ul ,li ,fieldset{margin: 0 ; padding: 0;}
/* use dark grey for the text, leaving black for headers etc */
body, td, th , tr,table {
    margin: 0;
    padding: 5px;
    text-align: left;
    vertical-align: top;
}
/* lets give all forms zero padding/margins */
form{padding: 0px;	margin: 0px;}
/* Input boxes - just a bit smaller than normal so they align well. */
input, textarea, button, select{	font: Verdana, Arial, Helvetica, sans-serif;border: 1px solid #d8d8d4;}


textarea{
    font-size: 100%;
    font-family: verdana, arial, tahoma,  sans-serif;
}
/* All input elements that are checkboxes or radio buttons. */
input.check{	border: none;}
/* By default set the color on these tags as black. */
h1, h2, h3, h4, h5, h6, h7{
    /* color: #dddddd;*/
    font-size: 1em;
    margin: 0;
    padding: 0;
    margin: 0 0 7px;
}
/* No image should have a border when linked. */
a img{	border: 0;}
/* No lines around buttons in Firefox! */
a{	outline: none;}
/* Reset all lists. */
ul, ul li{
    padding: 0;
    margin: 0;
    list-style: none;
}
ol, ul {
    list-style: none outside none;
}
/* ************************************* */
/* Some handy custom classes             */
/* ************************************* */
* html .clearfix { zoom: 1; } /* IE6 */
*:first-child+html .clearfix { zoom: 1; } /* IE7 */
/* Helping style to clear floated items. */
.clearfix:after{
    display: block;
    height: 0;
    clear: both;
    font-size: 0;
    content: " ";
    height: 0;
    visibility: hidden;
}
.clear {	clear: both;}
p {margin: 0;}

