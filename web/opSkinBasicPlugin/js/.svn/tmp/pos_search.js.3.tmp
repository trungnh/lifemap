    // hàm gọi ajax
    function ajaxCall(url, param, callback) {
        $.ajax({
            url: url,
            type: 'get',
            data: param,
            dataType: 'json',
            beforeSend: function() {
                showLoading();
            },
            success: function(data) {
                callback(data);
                hideLoading();
            },
            error: function(){
                hideLoading();
            }
        });
    }
