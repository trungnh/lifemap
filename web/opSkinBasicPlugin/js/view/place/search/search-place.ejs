<div id="search-container">
    <div id="search-place-category" class="categories">
        <ul>
        <% for(var i = 0; i < data.length; i++) { %>
            <li rel="<%=data[i].code%>" href="#<%=data[i].code%>" title="<%=data[i].title%>">
                <span><img width="40" src="images/category/<%=data[i].code%>.png" /></span>
                <h5><%=data[i].name%></h5>
            </li>
        <%}%>
        </ul>
    </div>
    
    <div id="search-info-board">
        <div class="content_container">
            <div id="left-col-display"></div>
        </div>
    </div>

    <div class="clear"></div>
</div>