<ul>
    <% for(var i=0; i<list.length; i++) { %>
        <li class="location-item" id="location-item-<%=list[i].id%>">
            <% if(list[i].file_id) { %>
                <img class='img_40x40' src="<%='../web/cache/img/jpg/w40_h40/'+ list[i].file_id + '_crop.jpg'%>" />
            <% } else { %>
                <img class='img_40x40' src="../web/images/no_image.gif" />
            <% } %>
            
            <div class="right-block">
                <div class="location-title">
                    <a href="pos/<%=list[i].id%>" class="" target='_blank'><%=list[i].title %></a>
                </div>

                <div class="location-address">
                  <%=list[i].address%>
                </div>
                <%if(list[i].numOfMember){%>
                    <div class="location-note">
                      <%=list[i].numOfMember%>
                    </div>
                <%}%>
                <%if(list[i].comment){%>
                    <div class="location-note">
                      <%=list[i].comment%>
                    </div>
                <%}%>
            </div>
            <a href="<%=list[i].address%>" class="start_way" ><%='Từ đây' %></a>
            <a href="<%=list[i].address%>" class="end_way" ><%='Đến đây' %></a>
            <div class="clear"></div>
        </li>
    <% } %>
</ul>