    <form id="house-photo-form">
        <div id="house-info-tab">
            <div id="house-info-fields">
                <div class="grouper">
                    <p class="house-info-form-title">Chú thích:</p>
                    <input name="renthouse-address"  id="photo-name" />

                    <p class="house-info-form-title">Ảnh:</p>
                    <div class="mView">
                     <div id="upload_box">
                        <div id="upload" class="ok-button" >Tải ảnh</div>
                        <span id="status" ></span>
                      </div>
                    </div>                   
                </div>
        </div>
        <div class="clear"></div>
    </form>
<%if(images){%>
<div class="grouper" id="list-photo-rent-house">
  <p class="house-info-form-title">Danh sách ảnh</p>
  <%for(i=0;i<images.length;i++){%>
      <div class='photo-new'>
          <img class='img_40x40' src="<%='../../web/cache/img/jpg/w40_h40/'+ images[i]['file_name'] + '_crop.jpg'%>" />
          <br/>
          <p><%=images[i]['description']%></p>
          <a class='delete_photo_pos ok-button' href="<%='deletePhotoPos?id='+images[i]['id']+'&file_id='+images[i]['file_id']%>">Xóa</a>
          <a class='avatar-post ok-button' href="<%='avatarEdit?fileId='+images[i]['file_id']+'&posId='+images[i]['pos_id']%>">Chọn làm ảnh đại điện</a>
          <div class="clear"></div>
      </div>
  <%}%>
</div>
<%}%>